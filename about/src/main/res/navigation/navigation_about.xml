<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_about"
    app:startDestination="@id/fragment_about">

    <include app:graph="@navigation/navigation_browser" />

    <fragment
        android:id="@+id/fragment_about"
        android:name="com.gdavidpb.tuindice.about.ui.fragments.AboutFragment"
        android:label="@string/nav_about"
        tools:layout="@layout/fragment_about">
        <action
            android:id="@+id/nav_to_browser"
            app:destination="@id/navigation_browser"
            app:enterAnim="@anim/slide_in_from_right"
            app:exitAnim="@anim/slide_out_to_left"
            app:popEnterAnim="@anim/slide_in_from_left"
            app:popExitAnim="@anim/slide_out_to_right">
            <argument
                android:name="title"
                app:argType="string" />
            <argument
                android:name="url"
                app:argType="string" />
        </action>
    </fragment>

</navigation>